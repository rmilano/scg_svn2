}

.ui-widget-separator
{
    background-color: #c6c7c8;
    border: 1px solid #565656;
    color: white;
    font-size: 10px;
    font-style: normal;
    font-variant: normal;
    font-weight: bold;
    line-height: normal;
    text-decoration: none;
    text-transform: none;
}

