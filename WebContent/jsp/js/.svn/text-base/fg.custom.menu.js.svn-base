$(function(){
    // BUTTONS
    $('.fg-button').hover(
        function(){
            $(this).removeClass('ui-state-default').addClass('ui-state-focus');
        },
        function(){
            $(this).removeClass('ui-state-focus').addClass('ui-state-default');
        }
        );
    $('#inicio').menu({
        content: $('#inicio').next().html(),
        flyOut: true
    });
    $('#asuntos').menu({
        content: $('#asuntos').next().html(),
        flyOut: true
    });
    $('#reportes').menu({
        content: $('#reportes').next().html(),
        flyOut: true
    });
    $('#admin').menu({
        content: $('#admin').next().html(),
        flyOut: true
    });
    $('#ayuda').menu({
        content: $('#ayuda').next().html(),
        flyOut: true
    });
});



