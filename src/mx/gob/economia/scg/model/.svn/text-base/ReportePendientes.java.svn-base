package mx.gob.economia.scg.model;

import java.util.ArrayList;
import java.util.List;

public class ReportePendientes
{
    private Integer total;
    private List<Asunto> asuntos;
    public ReportePendientes()
    {
        this.total = 0;
        this.asuntos = new ArrayList<Asunto>();
    }


    /**
     * @return the total
     */
    public Integer getTotal()
    {
        return total;
    }

    /**
     * @param total the total to set
     */
    public void setTotal(Integer total)
    {
        this.total = total;
    }

    public List<Asunto> getAsuntos() {
        return asuntos;
    }

    public void setAsuntos(List<Asunto> asuntos) {
        this.asuntos = asuntos;
    }
    
}
