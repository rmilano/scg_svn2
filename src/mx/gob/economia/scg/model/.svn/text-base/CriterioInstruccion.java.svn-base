package mx.gob.economia.scg.model;

import java.io.Serializable;

/**
 *
 * @author rodrigo.martinez
 */
public class CriterioInstruccion implements Serializable{
    private Paginador paginador;
    private Instruccion instruccion;
    
    public CriterioInstruccion(){
        paginador=new Paginador();
        instruccion=new Instruccion();
    }

    /**
     * @return the paginador
     */
    public Paginador getPaginador() {
        return paginador;
    }

    /**
     * @param paginador the paginador to set
     */
    public void setPaginador(Paginador paginador) {
        this.paginador = paginador;
    }

    /**
     * @return the instruccion
     */
    public Instruccion getInstruccion() {
        return instruccion;
    }

    /**
     * @param instruccion the instruccion to set
     */
    public void setInstruccion(Instruccion instruccion) {
        this.instruccion = instruccion;
    }
}
