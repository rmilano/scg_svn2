package mx.gob.economia.scg.form;

import java.util.ArrayList;
import java.util.List;
import mx.gob.economia.scg.model.CriterioInstruccion;
import mx.gob.economia.scg.model.Instruccion;


/**
 *
 * @author rodrigo.martinez
 */
public class InstruccionForm extends AreaForm{
    
       private Instruccion instruccion;
       private List<Instruccion> instrucciones;
       private CriterioInstruccion criterioInstruccion;
       
       public InstruccionForm(){
           super();
           instruccion=new Instruccion();
           instrucciones=new ArrayList<Instruccion>();
           criterioInstruccion=new CriterioInstruccion();
       }

    /**
     * @return the instruccion
     */
    public Instruccion getInstruccion() {
        return instruccion;
    }

    /**
     * @param instruccion the instruccion to set
     */
    public void setInstruccion(Instruccion instruccion) {
        this.instruccion = instruccion;
    }

    /**
     * @return the instrucciones
     */
    public List<Instruccion> getInstrucciones() {
        return instrucciones;
    }

    /**
     * @param instrucciones the instrucciones to set
     */
    public void setInstrucciones(List<Instruccion> instrucciones) {
        this.instrucciones = instrucciones;
    }

    /**
     * @return the criterio
     */
    public CriterioInstruccion getCriterioInstruccion() {
        return criterioInstruccion;
    }

    /**
     * @param criterio the criterio to set
     */
    public void setCriterioInstruccion(CriterioInstruccion criterioInstruccion) {
        this.criterioInstruccion = criterioInstruccion;
    }
    
}
