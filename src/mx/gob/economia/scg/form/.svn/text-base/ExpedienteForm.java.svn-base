package mx.gob.economia.scg.form;

import java.util.ArrayList;
import java.util.List;
import mx.gob.economia.scg.model.CriterioExpediente;
import mx.gob.economia.scg.model.Expediente;

/**
 *
 * @author rodrigo.martinez
 */
public class ExpedienteForm extends AreaForm{
    private Expediente expediente;
    private List<Expediente> expedientes;
    private CriterioExpediente criterioExpediente;
    
    public ExpedienteForm(){
        expediente=new Expediente();
        expedientes=new ArrayList<Expediente>();
        criterioExpediente=new CriterioExpediente();
    }

    /**
     * @return the expediente
     */
    public Expediente getExpediente() {
        return expediente;
    }

    /**
     * @param expediente the expediente to set
     */
    public void setExpediente(Expediente expediente) {
        this.expediente = expediente;
    }

    /**
     * @return the expedientes
     */
    public List<Expediente> getExpedientes() {
        return expedientes;
    }

    /**
     * @param expedientes the expedientes to set
     */
    public void setExpedientes(List<Expediente> expedientes) {
        this.expedientes = expedientes;
    }

    /**
     * @return the criterioExpediente
     */
    public CriterioExpediente getCriterioExpediente() {
        return criterioExpediente;
    }

    /**
     * @param criterioExpediente the criterioExpediente to set
     */
    public void setCriterioExpediente(CriterioExpediente criterioExpediente) {
        this.criterioExpediente = criterioExpediente;
    }
}
