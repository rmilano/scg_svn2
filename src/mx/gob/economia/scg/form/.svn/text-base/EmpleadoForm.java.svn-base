package mx.gob.economia.scg.form;

import java.util.ArrayList;
import java.util.List;
import mx.gob.economia.scg.model.CriterioEmpleado;
import mx.gob.economia.scg.model.Empleado;
import mx.gob.economia.scg.model.Rol;
import org.apache.struts.action.ActionForm;

/**
 *
 * @author roque
 */
public class EmpleadoForm extends ActionForm
{
    private Empleado empleadoCreado;
    private CriterioEmpleado criterioEmpleado;
    private List<Empleado> empleados;
    private List<Rol> roles;
    private Integer[] selectedRoles;
    
    public EmpleadoForm()
    {
        this.empleadoCreado = new Empleado();        
        this.empleados = new ArrayList<Empleado>();
        this.criterioEmpleado = new CriterioEmpleado();
        this.roles = new ArrayList<Rol>();
    }
    /**
     * @return the empleadoCreado
     */
    public Empleado getEmpleadoCreado()
    {
        return empleadoCreado;
    }

    /**
     * @param empleadoCreado the empleadoCreado to set
     */
    public void setEmpleadoCreado(Empleado empleadoCreado)
    {
        this.empleadoCreado = empleadoCreado;
    }

    /**
     * @return the empleados
     */
    public List<Empleado> getEmpleados()
    {
        return empleados;
    }

    /**
     * @param empleados the empleados to set
     */
    public void setEmpleados(List<Empleado> empleados)
    {
        this.empleados = empleados;
    }

    /**
     * @return the criterioEmpleado
     */
    public CriterioEmpleado getCriterioEmpleado()
    {
        return criterioEmpleado;
    }

    /**
     * @param criterioEmpleado the criterioEmpleado to set
     */
    public void setCriterioEmpleado(CriterioEmpleado criterioEmpleado)
    {
        this.criterioEmpleado = criterioEmpleado;
    }

    /**
     * @return the roles
     */
    public List<Rol> getRoles()
    {
        return roles;
    }

    /**
     * @param roles the roles to set
     */
    public void setRoles(List<Rol> roles)
    {
        this.roles = roles;
    }

    /**
     * @return the selectedRoles
     */
    public Integer[] getSelectedRoles()
    {
        return selectedRoles;
    }

    /**
     * @param selectedRoles the selectedRoles to set
     */
    public void setSelectedRoles(Integer[] selectedRoles)
    {
        this.selectedRoles = selectedRoles;
    }
    
}
