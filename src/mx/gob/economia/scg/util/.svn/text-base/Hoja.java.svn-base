package mx.gob.economia.scg.util;

import org.apache.log4j.Logger;

/**
 *
 * @author gerardo
 */
class Hoja extends Componente
{

    public Hoja(String nombre, Integer id)
    {
        super(nombre, id);
    }

    public void agregar(Componente c)
    {
        Logger log = Logger.getLogger(this.getClass());
        log.error("no se puede agregar la hoja");
    }

    public void remover(Componente c)
    {
        Logger log = Logger.getLogger(this.getClass());
        log.error("no se puede quitar la hoja");
    }

    public String mostrar(String profundidad)
    {
        Logger log = Logger.getLogger(this.getClass());
        log.debug('-' + "" + this.getNombre());
        return this.getNombre();
    }
}
